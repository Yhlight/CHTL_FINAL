html {
    head {
        title { text { "CHTL Test" } }
    }
    body {
        // This div already has a class and an id.
        // The style block should add a new class and overwrite the id.
        div {
            id: old-id;
            class: existing-class;

            style {
                #new-id { font-size: 16px; }
                .new-class { font-weight: bold; }
            }

            h1 {
                text { "Test" }
            }
        }
    }
}
