// Test/templates.chtl

[Template] @Style DefaultText
{
    color: black;
    font-size: 16px;
    line-height: 20px;
}

[Template] @Element Box
{
    div {
        class: "box-title";
        text: "This is a box";
    }
    div {
        class: "box-content";
    }
}

body {
    // Usage of Element Template
    @Element Box;

    div {
        id: "p1";
        style {
            @Style DefaultText;
        }
    }
}
