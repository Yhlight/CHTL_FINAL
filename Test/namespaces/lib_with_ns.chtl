// This library defines components within a namespace called 'forms'

[Namespace] forms {
    [Template] @Style InputStyle {
        border: "1px solid #999";
        padding: "5px";
    }

    [Custom] @Element LabeledInput {
        div {
            class: "form-group";
            label { text { "Default Label" } }
            input {
                style { @Style InputStyle; }
            }
        }
    }
}
