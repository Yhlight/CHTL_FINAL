[Template] @Style DefaultText {
    font-size: 16px;
}

[Template] @Element Card {
    div {
        style {
            @Style DefaultText;
            border: 1px solid black;
        }
        text: "This is a card";
    }
}

html {
    body {
        // Use the element template multiple times
        @Element Card;
        @Element Card;
    }
}
