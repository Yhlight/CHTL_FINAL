div {
    style {
        width: 100px + 50px;           // Addition
        height: 200px - 25px;          // Subtraction
        font-size: 10px * 2;           // Multiplication
        margin: 100px / 4;             // Division
        padding: 11px % 5px;           // Modulo
        border-width: 2px ** 3px;      // Power
        left: (50% + 10%) * 2;         // Grouping
    }
}
