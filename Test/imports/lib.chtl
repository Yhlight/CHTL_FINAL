// This is the library file

[Template] @Style ButtonStyle {
    border: "1px solid #ccc";
    padding: "8px 16px";
    border-radius: "4px";
}

[Custom] @Element Card {
    div {
        class: "card";
        h4 {
            class: "card-title";
            text { "Default Title" }
        }
        p {
            class: "card-content";
            text { "Default content." }
        }
    }
}
