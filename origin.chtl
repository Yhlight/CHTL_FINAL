html {
    head {
        [Origin] @Style {
            body { font-family: sans-serif; }
        }
    }
    body {
        h1 { text: "Origin Test"; }

        [Origin] @Html {
            <p>This paragraph is raw HTML.</p>
        }

        [Origin] @JavaScript {
            console.log("Hello from an Origin block!");
        }
    }
}
