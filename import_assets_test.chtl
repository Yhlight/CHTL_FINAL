[Import] @Html from "test.html" as my_html_content;
[Import] @Style from "test.css" as my_css_rules;

html {
    head {
        style {
            // This should embed the content of test.css
            [Origin] @Style my_css_rules;
        }
    }
    body {
        // This should embed the content of test.html
        [Origin] @Html my_html_content;
    }
}
