<!DOCTYPE html>
<html>
<head>
    <title>Basic Template Style Test</title>
</head>
<body>
    <h1>Basic Template Style Test</h1>
    
    [Template] @Style MyButton {
        background-color: #007bff;
        color: white;
        padding: 10px 20px;
        border: none;
        border-radius: 4px;
        font-size: 14px;
        cursor: pointer;
    }
    
    [Template] @Style PrimaryButton {
        background-color: #28a745;
        color: white;
        padding: 12px 24px;
        border: none;
        border-radius: 6px;
        font-size: 16px;
        font-weight: bold;
    }
    
    [Origin] @Html {
        <div class="container">
            <button class="MyButton">Click Me</button>
            <button class="PrimaryButton">Primary Button</button>
        </div>
    }
</body>
</html>