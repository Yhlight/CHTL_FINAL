[Template] @Style MyButton {
    color: red;
    background-color: blue;
    border: 1px solid black;
}

[Template] @Element Card {
    class: "card";
    style: "padding: 20px; margin: 10px;";
}

[Template] @Var Theme {
    primary-color: "#007bff";
    secondary-color: "#6c757d";
    font-family: "Arial, sans-serif";
}