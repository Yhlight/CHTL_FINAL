[Template] @Element MyContent {
    h2 { text { "Original Title" } }
    p { text { "Original paragraph." } }
}

html {
    body {
        @Element MyContent {
            insert before h2[0] {
                h1 { text { "New Main Title" } }
            }

            insert after p[0] {
                hr;
                p { text { "An inserted paragraph." } }
            }
        }
    }
}
