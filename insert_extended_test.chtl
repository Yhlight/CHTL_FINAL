[Template] @Element TestBlock {
    div { id: "one"; }
    div { id: "two"; }
    div { id: "three"; }
}

html {
    body {
        // Test 'at top'
        @Element TestBlock {
            insert at top {
                p { text: "Should be first"; }
            }
        }

        // Test 'at bottom'
        @Element TestBlock {
            insert at bottom {
                p { text: "Should be last"; }
            }
        }

        // Test 'replace'
        @Element TestBlock {
            replace div[1] {
                span { text: "I replaced div two"; }
            }
        }
    }
}
