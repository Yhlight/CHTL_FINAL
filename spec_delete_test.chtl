[Template] @Element DeletableContent {
    h1 { text: "Title"; }
    p { text: "Paragraph 1"; }
    p { text: "Paragraph 2"; }
    span { text: "A span"; }
}

body {
    // Test deleting the first p tag
    @Element DeletableContent {
        delete p[0];
    }

    // Test deleting the span tag by type
    @Element DeletableContent {
        delete span;
    }
}
