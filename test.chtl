[Template] @Element MyContent {
    h1 { text { "Title" } }
    p { text { "First paragraph." } }
    hr;
    p { text { "Second paragraph." } }
}

html {
    body {
        @Element MyContent {
            // Delete the horizontal rule
            delete hr;
            // Delete the second paragraph (index 1)
            delete p[1];
        }
    }
}
