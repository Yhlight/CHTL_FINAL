[Configuration] {
    INDEX_INITIAL_COUNT = 1;
}

[Template] @Element MyList {
    item { text { "A" } } // Index 1
    item { text { "B" } } // Index 2
    item { text { "C" } } // Index 3
}

html {
    body {
        @Element MyList {
            // With INDEX_INITIAL_COUNT = 1, this should target the first item.
            item[1] {
                style {
                    font-weight: bold;
                }
            }
        }
    }
}
