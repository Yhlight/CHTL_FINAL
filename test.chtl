// Define an element template
[Template] @Element Card
{
    div {
        class: "card";
        h2 { text { "Card Title" } }
        p { text { "This is the content of the card." } }
    }
}

html
{
    head
    {
        title { text { "Element Template Test" } }
    }

    body
    {
        h1 { text { "My Page" } }

        // Use the element template
        @Element Card;

        hr {}

        // Use it again
        @Element Card;
    }
}
