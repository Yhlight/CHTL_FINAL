[Template] @Style DefaultText {
    color: black;
    font-size: 16px;
}

[Custom] @Style CustomText {
    color: blue;
}

[Template] @Element Box {
    div {
        text { "This is a box from a template." }
    }
}

html {
    head {
        title { text { "CHTL Page from File" } }
    }
    body {
        div {
            id = my-div;
            class: 'container';
            style {
                @Style DefaultText;
                background-color: blue;
            }
            text { "Hello from a CHTL file!" }
        }

        @Element Box {
            delete div;
        }
    }
}
