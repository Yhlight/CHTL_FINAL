html
{
    head
    {
        title { text { "Context Deduction Test" } }
    }

    body
    {
        // This div should get class="box" and have a hover effect.
        div
        {
            style {
                .box {
                    color: blue;
                }

                &:hover {
                    color: red;
                    text-decoration: underline;
                }
            }

            text { "This text should turn red on hover." }
        }
    }
}
