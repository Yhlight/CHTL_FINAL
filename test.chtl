use html5;

[Import] @Style from "styles.css";

[Template] @Element Box
{
    div { text: "first"; }
    div { text: "second"; }
    p { text: "a paragraph"; }
}

[Namespace] mySpace
{
    [Template] @Element Box
    {
        h1 { text: "This is the Box from mySpace"; }
    }
}

body
{
    // Test qualified name resolution
    @Element Box from mySpace;

    // Test template specialization
    @Element Box
    {
        delete div[0]; // Should delete "first"
        except p;      // This should cause an error!
    }
}
