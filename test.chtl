[Custom] @Element MyBox {
    div {
        class: "original-class";
        p { text: "Hello"; }
    }
}

body {
    @Element MyBox {
        div { // This div matches the div in the template
            style { border: 1px solid red; }
            p {   // This p matches the p inside the template's div
                style { color: blue; }
            }
        }
    }
}
